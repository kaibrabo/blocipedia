<div class="jumbotron">
    <h1>All Wikis</h1>

    <div class=" center-block">
        <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
    </div>
    <br />
    <br />
        <% @wikis.each do |wiki| %>
            <div class="media">
                <div class="media-body">
                    <h4 class="media-heading">
                        <%= link_to markdown(wiki.title), wiki %>
                    </h4>
                </div>
            </div>
        <% end %>

</div>
